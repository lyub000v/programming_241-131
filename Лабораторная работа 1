#include <iostream> 
#include <clocale>
using namespace std;

int main() {
    setlocale(LC_ALL, "Russian");
    cout << "First string" << endl;
    cout << "Первая строка\nВторая строка" << endl;
    cout << "Спецсимволы \"\\\"" << endl;
    return 0;

}   


#include <clocale>
#include <iostream>
using namespace std;

int main() {
    setloca-le(LC_ALL, "Russian");
    int cl = 150;
    float fl = 15.933;
    unsigned char tt = 250;
    cout << "cl = " << cl << endl;
    cout << "fl = " << fl << endl;
    cout << "tt = " << tt << endl;

    int d = 15;           
    string m = "Май";   
    int y = 1990;        


  
    cout << "Моя дата рождения: " << d << " " << m << " " << y << " года" << endl;

  
    const float cf = 2.3;
    const string cs = "WINDOWS";


    cout << cf << " " << cs << endl;

}
