#include <iostream> 
#include <clocale>
int main() {          
    setlocale(LC_ALL, "Russian");
    std::cout << "First string" << std::endl;
    std::cout << "Первая строка\nВторая строка" << std::endl;
    std::cout << "Спецсимволы \"\\\"" << std::endl;
    return 0;

}     


#include <clocale>
#include <iostream>
using namespace std;

int main() {
    setlocale(LC_ALL, "Russian");
    int intVar = 150;
    float floatVar = 15.933;
    unsigned char byteVar = 250;
    cout << "intVar = " << intVar << endl;
    cout << "floatVar = " << floatVar << endl;
    cout << "byteVar = " << byteVar << endl;

    int day = 15;           
    string month = "Май";   
    int year = 1990;        


  
    cout << "Моя дата рождения: " << day << " " << month << " " << year << " года" << endl;

  
    const float constantFloat = 2.3;
    const string constantString = "WINDOWS";


    cout << constantFloat << " " << constantString << endl;

}
